{
  "name": "@atriumn/sdk-js",
  "version": "0.1.0",
  "description": "MCP client SDK for web and mobile apps in the Atriumn ecosystem",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "atriumn",
    "mcp",
    "model-context-protocol",
    "websocket",
    "sdk",
    "javascript",
    "typescript",
    "client",
    "web-sdk",
    "mobile-sdk"
  ],
  "author": "Atriumn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/atriumn/sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/atriumn/sdk-js/issues"
  },
  "homepage": "https://github.com/atriumn/sdk-js#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "jsdom": "^26.1.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "ws": "^8.14.0"
  },
  "peerDependencies": {
    "@types/ws": "^8.5.0"
  }
}
